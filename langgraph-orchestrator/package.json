{
    "name": "langgraph-orchestrator",
    "version": "1.0.0",
    "description": "A langgraph orchestrator for AI Applications",
    "main": "dist/index.js",
    "directories": {
        "test": "tests"
    },
    "type": "module",
    "scripts": {
        "start": "node --experimental-loader ./json-loader.mjs dist/index.js",
        "dev": "npm run build && node --experimental-loader ./json-loader.mjs dist/index.js",
        "build": "tsc && chmod 755 dist/index.js",
        "prestart": "npm run build",
        "clean": "rimraf dist",
        "lint": "eslint 'src/**/*.{ts,js}'",
        "test": "jest"
    },
    "dependencies": {
        "@langchain/core": "^0.3.42",
        "@langchain/langgraph": "^0.2.57",
        "@langchain/mcp-adapters": "^0.3.1",
        "@langchain/openai": "^0.4.5",
        "dotenv": "^16.4.7",
        "eventsource": "^3.0.5",
        "express": "^4.21.2",
        "extended-eventsource": "^2.1.0"
    },
    "devDependencies": {
        "@types/body-parser": "^1.19.5",
        "@types/dotenv": "^6.1.1",
        "@types/express": "^4.17.13",
        "@types/node": "^16.11.7",
        "esbuild": "^0.25.1",
        "ts-node": "^10.4.0",
        "ts-node-dev": "^1.1.8",
        "typescript": "^4.9.5"
    },
    "author": "creai"
}
